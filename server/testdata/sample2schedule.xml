<schedule name="samp2schedule" timing="daily" >

        <event name="first event" receive-by="11:09:00">
                <error to="email handler"/>
        </event>


        <event name="second event" receive-by="15:09:00">
                <error to="email handler"/>
        </event>

        <event name="third event" receive-by="16:00:00">
                <error to="email handler"/>
        </event>


        <handler name="WEBHOOK">
                <address>http://localhost:8081/api/v1/msg</address>
        </handler>

</schedule>

